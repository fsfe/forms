{
  "contact-form": {
    "plain": {
      "content": "{{ content }}"
    },
    "required_vars": [
      "content"
    ]
  },
  "pmpc-sign": {
    "plain": {
       "filename": "pmpc-sign.txt"
    },
    "required_vars": [
      "name"
    ]
  },
  "pmpc-confirm": {
    "plain": {
       "filename": "pmpc-confirm.txt"
    }
  },
  "art13-presignup-confirm": {
    "plain": {
       "filename": "art13-presignup-confirm.txt"
    }
  },
  "art13-presignup": {
    "plain": {
       "content": "New pre-signup: {{ name }} {{ confirm }}"
    },
    "required_vars": [
       "name",
       "confirm",
       "optin"
    ]
  },
  "reuse-signup": {
    "plain": { "content": "{{ confirm }};{{ optin }}" },
    "required_vars": [ "confirm", "optin" ]
  },
  "reuse-signup-confirm": {
    "plain": { "filename": "reuse-signup-confirm.txt" }
  },
  "ln-apply": {
    "filename": "ln-apply.txt",
    "required_vars": ["applicant_name"],
    "headers": {
      "X-APPLICANT-NAME": "{{ applicant_name }}"
    }
  }
}
