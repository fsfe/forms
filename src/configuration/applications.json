{
  "contact": {
    "to": [
      "contact@fsfe.org"
    ],
    "include_vars": true,
    "redirect": "https://fsfe.org/contact/contact-thankyou",
    "template": "contact-form"
  },
  "art13contact": {
    "to": [
      "contact@fsfe.org"
    ],
    "include_vars": true,
    "redirect": "https://savecodeshare.eu/thank-you",
    "template": "contact-form"
  },
  "art13-presignup": {
    "from": "no-reply@fsfe.org",
    "to": [
      "jonas@fsfe.org",
      "diana@openforumeurope.org"
    ],
    "include_vars": true,
    "confirm": true,
    "redirect": "https://savecodeshare.eu/thank-you-signup",
    "redirect-confirmed": "https://savecodeshare.eu/thank-you-confirmed",
    "store": "/store/art13/presignup.json",
    "subject": "New signup to savecodeshare",
    "confirmation-template": "art13-presignup-confirm",
    "confirmation-subject": "Just one step left! Confirm your email to save code share!",
    "template": "art13-presignup"
  },
  "pmpc-sign": {
    "from": "admin@fsfe.org",
    "to": [ "contact@fsfe.org" ],
    "headers": {
      "X-OTRS-Queue": "PMPC"
    },
    "subject": "New signature to PMPC",
    "include_vars": true,
    "confirm": true,
    "redirect": "https://publiccode.eu/openletter/confirm",
    "redirect-confirmed": "https://publiccode.eu/openletter/success",
    "template": "pmpc-sign",
    "confirmation-template": "pmpc-confirm",
    "confirmation-subject": "Public Code: Please confirm your signature",
    "store": "/store/pmpc/signatures.json"
  },
  "reuse-signup": {
    "from": "no-reply@fsfe.org",
    "to": [ "jonas@fsfe.org" ],
    "subject": "New signup to REUSE",
    "confirm": true,
    "include_vars": true,
    "redirect": "https://reuse.software/sign-confirm",
    "redirect-confirmed": "https://reuse.software/sign-success",
    "template": "reuse-signup",
    "store": "/store/reuse/signups.json",
    "confirmation-template": "reuse-signup-confirm",
    "confirmation-subject": "Just one step left! Confirm your email for REUSE!"
  },
  "ln-apply": {
    "from": "no-reply@fsfe.org",
    "to": [ "tech@mehl.mx" ],
    "replyto": "{{ applicant_email }}",
    "subject": "Application for Legal Network membership by {{ applicant_name }}",
    "include_vars": true,
    "redirect": "https://fsfe.org/activities/ftf/ln.en.html",
    "template": "ln-apply",
    "headers": {
      "X-OTRS-Queue": "Promo",
      "X-OTRS-VAR": "TEST"
    },
    "store": "/store/ln/applications.json"
  }
}
